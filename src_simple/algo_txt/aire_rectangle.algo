// Calcul de l'aire d'un rectangle
// ******************************************

VARIABLES
  largeur EST_DU_TYPE NOMBRE
  longueur EST_DU_TYPE NOMBRE
DEBUT_ALGORITHME
  // Lecture des variables
  LIRE largeur
  LIRE longueur
  //Affiche le r√©sultat
  AFFICHER "Aire du rectangle : "
  AFFICHERCALCUL largeur * longueur
FIN_ALGORITHME
